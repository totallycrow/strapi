{
  "kind": "collectionType",
  "collectionName": "comic_posts",
  "info": {
    "singularName": "comic-post",
    "pluralName": "comic-posts",
    "displayName": "ComicPost",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "Blocks": {
      "type": "dynamiczone",
      "components": [
        "comics.text-block",
        "comics.image-block"
      ]
    },
    "excerpt": {
      "type": "text"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "comic_posts"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "comic_posts"
    },
    "description": {
      "type": "text"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "generic.seo"
    },
    "heroImage": {
      "type": "component",
      "repeatable": false,
      "component": "generic.hero-image"
    },
    "datePublished": {
      "type": "datetime"
    },
    "dateModified": {
      "type": "datetime"
    }
  }
}
