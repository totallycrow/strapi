{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "excerpt": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "posts"
    },
    "datePublished": {
      "type": "datetime"
    },
    "dateModified": {
      "type": "datetime"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "posts"
    },
    "showcaseDescription": {
      "type": "text"
    },
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "generic.seo"
    },
    "heroImage": {
      "type": "component",
      "repeatable": false,
      "component": "generic.hero-image"
    },
    "contentArea": {
      "type": "dynamiczone",
      "components": [
        "generic.video-embed",
        "generic.image-block",
        "generic.gif-embed",
        "generic.content-block",
        "generic.adsense-block"
      ]
    }
  }
}
